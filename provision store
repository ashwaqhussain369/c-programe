#include<stdio.h>
int main()
{
    int rice_quantity,rice_price,sugar_quantity,sugar_price,oil_quantity,oil_price,total,tax_amount,final_amount;
    printf("rice_quantity=");
    scanf("%d",&rice_quantity);
    printf("rice_price=");
    scanf("%d",&rice_price);
    printf("sugar_quantity=");
    scanf("%d",&sugar_quantity);
    printf("sugar_price=");
    scanf("%d",&sugar_price);
    printf("oil_quantity=");
    scanf("%d",&oil_quantity);
    printf("oil_price=");
    scanf("%d",&oil_price);
    total=(rice_quantity*rice_price)+(sugar_quantity*sugar_price)+(oil_quantity*oil_price);
    tax_amount=total*5/100;
    final_amount=total+tax_amount;
    printf("the final amount is=%d",final_amount);
    return 0;
}
OUTPUT:
rice_quantity=2
rice_price=50
sugar_quantity=3
sugar_price=100
oil_quantity=1
oil_price=150
the final amount is=577
=== Code Execution Successful ===
